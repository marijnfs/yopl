# A random block, to contain scope / control lifetime of vars
[
   a[10] #a gets allocated (i64 is default)
] #a gets deleted

a, b -> calc -> c
   a = 4 * a
   a > 3 ?
      c = a
   |
      c = f
   ]
]

a -> bla -> c, d
   c = a * 3
   d = 0
]

